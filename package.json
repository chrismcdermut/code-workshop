{
  "name": "code-workshop",
  "version": "1.0.0",
  "description": "Where developers forge their skills - 80+ challenges, data structures, and algorithms",
  "main": "algorithm.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "jest --config jest.config.js",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "binarySearch": "node -e 'require(\"./algorithms/binarySearch/binarySearch\").runBinarySearch()'",
    "bubbleSort": "node -e 'require(\"./algorithms/bubbleSort/bubbleSort\").runBubbleSort()'",
    "circleArea": "node -e 'require(\"./misc/jsOOPConcepts\").circleArea()'",
    "connectFour:oop": "npx ts-node challenges/connectFour/oop/runConnectFourOOP.js",
    "connectFour:functional": "npx ts-node challenges/connectFour/functional/runConnectFourFunctional.js",
    "curryMultiply": "node -e 'require(\"./misc/jsOOPConcepts\").curryMultiply()'",
    "debounce": "node -e 'require(\"./challenges/debounce/debounce\").runDebounce()'",
    "domLoaded": "node -e 'require(\"./misc/jsOOPConcepts\").domLoaded()'",
    "generateBoilerPlate": "node -e 'require(\"./helpers/generateBoilerPlate\").generateDefaultBoilerPlate()'",
    "generate": "node generate.js",
    "heapSort": "node -e 'require(\"./algorithms/heapSort/heapSort\").runHeapSort()'",
    "iife": "node -e 'require(\"./misc/jsOOPConcepts\").iife()'",
    "iifeWindow": "node -e 'require(\"./misc/jsOOPConcepts\").iifeWindow()'",
    "insertionSort1": "node -e 'require(\"./algorithms/insertionSort/insertionSort\").runInsertionSort1()'",
    "insertionSort2": "node -e 'require(\"./algorithms/insertionSort/insertionSort\").runInsertionSort2()'",
    "jumpToZero": "node -e 'require(\"./challenges/arrayJumping/arrayJumping\").runJumpToZero()'",
    "mergeSort": "node -e 'require(\"./algorithms/mergeSort/mergeSort\").runMergeSort()'",
    "printHello": "node -e 'require(\"./misc/jsOOPConcepts\").printHello()'",
    "quickSort": "node -e 'require(\"./algorithms/quickSort/quickSort\").runQuickSort()'",
    "reverseWords": "node -e 'require(\"./challenges/reverseWords/reverseWords\").reverseWords()'",
    "selectionSort": "node -e 'require(\"./algorithms/selectionSort/selectionSort\").runSelectionSort()'",
    "seniorAge": "node -e 'require(\"./misc/jsOOPConcepts\").seniorAge()'",
    "yakovChain": "node -e 'require(\"./misc/jsOOPConcepts\").runYakovChain()'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrismcdermut/code-workshop.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrismcdermut/code-workshop/issues"
  },
  "homepage": "https://github.com/chrismcdermut/code-workshop#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "codecov": "^3.7.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "jest": "^25.4.0",
    "prettier": "2.0.5",
    "ts-jest": "^25.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "quadratic-solver": "^0.2.4"
  }
}
